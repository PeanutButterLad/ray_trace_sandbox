     RowBox[{"gw", "=", "50"}], ";", "\[IndentingNewLine]", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"Abs", "[", "img", "]"}], ">", "gw"}], ",", 
