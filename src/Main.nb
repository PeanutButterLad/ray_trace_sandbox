Cell[BoxData[
 RowBox[{
  RowBox[{"SingleRayGraphic", "[", 
   RowBox[{
   "source_List", ",", "image_List", ",", "\[Theta]_", ",", "showImg_"}], 
   "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
