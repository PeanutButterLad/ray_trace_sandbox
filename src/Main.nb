      RowBox[{"{", 
       RowBox[{"intx", ",", "inty"}], "}"}], "=", 
      RowBox[{"LensIntercept", "[", 
       RowBox[{"source", ",", "\[Theta]"}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{"obj", ",", "h1"}], "}"}], "=", "source"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{"img", ",", "h2"}], "}"}], "=", "image"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"gh", "=", 
      RowBox[{"Abs", "[", "inty", "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"Abs", "[", "h1", "]"}], ">", "gh"}], ",", 
       RowBox[{"gh", "=", 
        RowBox[{"Abs", "[", "h1", "]"}]}]}], "]"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"Abs", "[", "h2", "]"}], ">", "gh"}], ",", 
       RowBox[{"gh", "=", 
        RowBox[{"Abs", "[", "h2", "]"}]}]}], "]"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"gw", "=", 
      RowBox[{"Abs", "[", "obj", "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"Abs", "[", "img", "]"}], ">", "gw"}], ",", 
       RowBox[{"gw", "=", 
        RowBox[{"Abs", "[", "img", "]"}]}]}], "]"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"gw", "=", 
      RowBox[{"gw", "*", "1.5"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"lw", "=", 
      RowBox[{"gw", "/", "40"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"Graphics", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"LightOrange", ",", 
         RowBox[{"Rectangle", "[", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{
             RowBox[{"-", "gw"}], ",", 
             RowBox[{
              RowBox[{"-", "gh"}], "-", "2"}]}], "}"}], ",", 
           RowBox[{"{", 
            RowBox[{"gw", ",", 
             RowBox[{"gh", "+", "2"}]}], "}"}]}], "]"}], ",", "LightBlue", 
         ",", 
         RowBox[{"Rectangle", "[", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{
             RowBox[{"-", "lw"}], ",", 
             RowBox[{
              RowBox[{"-", "gh"}], "-", "1"}]}], "}"}], ",", 
           RowBox[{"{", 
            RowBox[{"lw", ",", 
             RowBox[{"gh", "+", "1"}]}], "}"}]}], "]"}], ",", "Black", ",", 
         RowBox[{"Line", "[", 
          RowBox[{"{", 
           RowBox[{
            RowBox[{"{", 
             RowBox[{
              RowBox[{"-", "obj"}], ",", "h1"}], "}"}], ",", 
            RowBox[{"{", 
             RowBox[{"intx", ",", "inty"}], "}"}], ",", 
            RowBox[{"{", 
             RowBox[{"img", ",", "h2"}], "}"}]}], "}"}], "]"}], ",", "Blue", 
         ",", "Dashed", ",", 
