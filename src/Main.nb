         RowBox[{"Arrowheads", "[", "Small", "]"}], ",", 
         RowBox[{"Arrow", "[", 
          RowBox[{"{", 
           RowBox[{
            RowBox[{"{", 
             RowBox[{"img", ",", "0"}], "}"}], ",", 
            RowBox[{"{", 
             RowBox[{"img", ",", "h2"}], "}"}]}], "}"}], "]"}]}], "}"}], ",", 
       
       RowBox[{"Axes", "\[Rule]", "True"}], ",", " ", 
       RowBox[{"AspectRatio", "->", 
        RowBox[{"1", "/", "2"}]}], ",", 
       RowBox[{"ImageSize", "->", "Full"}]}], "]"}]}]}], 
