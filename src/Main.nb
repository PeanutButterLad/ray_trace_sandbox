Cell[BoxData[{
 RowBox[{
  RowBox[{"SingleRayGraphic", "[", 
   RowBox[{"source_List", ",", "image_List", ",", "\[Theta]_"}], "]"}], ":=", 
  
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "obj", ",", "h1", ",", "img", ",", "h2", ",", "intx", ",", "inty", ",", 
      "gh", ",", "gw", ",", "lw"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
