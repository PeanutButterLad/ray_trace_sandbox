     RowBox[{"Graphics", "[", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"LightOrange", ",", 
         RowBox[{"Rectangle", "[", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{
             RowBox[{"-", "gw"}], ",", 
             RowBox[{"-", "gh"}]}], "}"}], ",", 
           RowBox[{"{", 
            RowBox[{"gw", ",", "gh"}], "}"}]}], "]"}], ",", 
         "\[IndentingNewLine]", "LightBlue", ",", 
         RowBox[{"Rectangle", "[", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{
             RowBox[{"-", "lw"}], ",", 
             RowBox[{
              RowBox[{"-", "gh"}], "-", "1"}]}], "}"}], ",", 
           RowBox[{"{", 
            RowBox[{"lw", ",", 
             RowBox[{"gh", "+", "1"}]}], "}"}]}], "]"}], ",", 
         "\[IndentingNewLine]", "Black", ",", "Thin", ",", 
         RowBox[{"Line", "[", 
          RowBox[{"{", 
           RowBox[{
            RowBox[{"{", 
             RowBox[{"obj", ",", "h1"}], "}"}], ",", 
            RowBox[{"{", 
             RowBox[{"intx", ",", "inty"}], "}"}]}], "}"}], "]"}], ",", 
         "\[IndentingNewLine]", "Black", ",", 
         RowBox[{"HalfLine", "[", 
          RowBox[{"{", 
           RowBox[{
            RowBox[{"{", 
             RowBox[{"intx", ",", "inty"}], "}"}], ",", 
            RowBox[{"{", 
             RowBox[{"img", ",", "h2"}], "}"}]}], "}"}], "]"}], ",", 
         "\[IndentingNewLine]", "Blue", ",", 
         RowBox[{"Arrowheads", "[", "Small", "]"}], ",", 
         RowBox[{"Arrow", "[", 
          RowBox[{"{", 
           RowBox[{
            RowBox[{"{", 
             RowBox[{"obj", ",", "0"}], "}"}], ",", 
            RowBox[{"{", 
             RowBox[{"obj", ",", "h1"}], "}"}]}], "}"}], "]"}], ",", 
