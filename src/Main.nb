      RowBox[{"{", 
       RowBox[{"obj", ",", "h1"}], "}"}], "=", "source"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{"img", ",", "h2"}], "}"}], "=", "image"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"gh", "=", "50"}], ";", "\[IndentingNewLine]", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"Abs", "[", "h1", "]"}], ">", "gh"}], ",", 
       RowBox[{"gh", "=", 
        RowBox[{
         RowBox[{"Abs", "[", "h1", "]"}], "+", "20"}]}]}], "]"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"Abs", "[", "h2", "]"}], ">", "gh"}], ",", 
       RowBox[{"gh", "=", 
        RowBox[{
