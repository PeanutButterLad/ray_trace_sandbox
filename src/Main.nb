Cell[BoxData[
 RowBox[{
  RowBox[{"RayDiagramGraphic", "[", 
   RowBox[{"source_List", ",", "plain_List"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "obj", ",", "h1", ",", "img", ",", "h2", ",", "x1", ",", "y1", ",", "x2",
       ",", "y2", ",", "x3", ",", "y3", ",", "gh", ",", "gw", ",", "lw"}], 
     "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{
      RowBox[{"{", 
       RowBox[{"obj", ",", "h1"}], "}"}], "=", "source"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{"img", ",", "h2"}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"x1", ",", "y1"}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"x2", ",", "y2"}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"x3", ",", "y3"}], "}"}]}], "}"}], "=", "plain"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"gh", "=", 
      RowBox[{"Abs", "[", "h1", "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"Abs", "[", "h2", "]"}], ">", "gh"}], ",", 
       RowBox[{"gh", "=", 
        RowBox[{"Abs", "[", "h2", "]"}]}]}], "]"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"gw", "=", 
      RowBox[{"Abs", "[", "obj", "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"Abs", "[", "img", "]"}], ">", "gw"}], ",", 
       RowBox[{"gw", "=", 
        RowBox[{"Abs", "[", "img", "]"}]}]}], "]"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"gw", "=", 
      RowBox[{"gw", "*", "1.5"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"lw", "=", 
      RowBox[{"gw", "/", "40"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"Graphics", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"LightOrange", ",", 
         RowBox[{"Rectangle", "[", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{
             RowBox[{"-", "gw"}], ",", 
             RowBox[{
              RowBox[{"-", "gh"}], "-", "2"}]}], "}"}], ",", 
           RowBox[{"{", 
            RowBox[{"gw", ",", 
             RowBox[{"gh", "+", "2"}]}], "}"}]}], "]"}], ",", "LightBlue", 
         ",", 
         RowBox[{"Rectangle", "[", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{
             RowBox[{"-", "lw"}], ",", 
             RowBox[{
              RowBox[{"-", "gh"}], "-", "1"}]}], "}"}], ",", 
           RowBox[{"{", 
            RowBox[{"lw", ",", 
             RowBox[{"gh", "+", "1"}]}], "}"}]}], "]"}], ",", "Black", ",", 
         RowBox[{"Line", "[", 
          RowBox[{"{", 
           RowBox[{
            RowBox[{"{", 
             RowBox[{
              RowBox[{"-", "obj"}], ",", "h1"}], "}"}], ",", 
            RowBox[{"{", 
             RowBox[{"x1", ",", "y1"}], "}"}], ",", 
            RowBox[{"{", 
             RowBox[{"img", ",", "h2"}], "}"}]}], "}"}], "]"}], ",", "Black", 
         ",", 
         RowBox[{"Line", "[", 
          RowBox[{"{", 
           RowBox[{
            RowBox[{"{", 
             RowBox[{
              RowBox[{"-", "obj"}], ",", "h1"}], "}"}], ",", 
            RowBox[{"{", 
             RowBox[{"x2", ",", "y2"}], "}"}], ",", 
            RowBox[{"{", 
             RowBox[{"img", ",", "h2"}], "}"}]}], "}"}], "]"}], ",", "Black", 
         ",", 
         RowBox[{"Line", "[", 
          RowBox[{"{", 
           RowBox[{
            RowBox[{"{", 
             RowBox[{
              RowBox[{"-", "obj"}], ",", "h1"}], "}"}], ",", 
            RowBox[{"{", 
             RowBox[{"x3", ",", "y3"}], "}"}], ",", 
            RowBox[{"{", 
             RowBox[{"img", ",", "h2"}], "}"}]}], "}"}], "]"}], ",", "Blue", 
         ",", "Dashed", ",", 
         RowBox[{"Arrowheads", "[", "Small", "]"}], ",", 
         RowBox[{"Arrow", "[", 
          RowBox[{"{", 
           RowBox[{
            RowBox[{"{", 
             RowBox[{
              RowBox[{"-", "obj"}], ",", "0"}], "}"}], ",", 
            RowBox[{"{", 
             RowBox[{
              RowBox[{"-", "obj"}], ",", "h1"}], "}"}]}], "}"}], "]"}], ",", 
